  "keywords": [
    "tailwind",
    "config",
    "corellium"
  ],
  "devDependencies": {
    "@haydenbleasel/harmony": "^1.0.20",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "auto": "^10.37.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.0",
    "rollup-plugin-typescript2": "^0.32.1"
  },
  "dependencies": {
